{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "buildCommand": "chmod +x build.sh && ./build.sh"
  },
  "deploy": {
    "startCommand": "export JAVA_HOME=$(dirname $(dirname $(which java))) && java -Dserver.port=$PORT -Dspring.profiles.active=prod $JAVA_OPTS -jar target/*.jar"
  }
}
